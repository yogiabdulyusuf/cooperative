			<menuitem	id="savings_deposit"	name="Deposit" parent="front_office_savings_group"	action="act_deposit_view" />
			<menuitem	id="savings_withdrawal"	name="Withdrawal" parent="front_office_savings_group"	action="act_withdrawal_view" />


